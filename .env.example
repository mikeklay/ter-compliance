# Database Configuration
SQLALCHEMY_DATABASE_URI=mysql+pymysql://username:password@your-rds-endpoint:3306/database_name
# For local development, use: sqlite:///compliance.sqlite3

# Security Keys (Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))")
SECRET_KEY=your-secret-key-here
JWT_SECRET=your-jwt-secret-here

# AWS S3 Configuration (Required for file uploads)
AWS_S3_BUCKET=your-s3-bucket-name
AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
